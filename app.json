{
  "expo": {
    "name": "عين",
    "slug": "3een",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "scheme": "een",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#FBF5EA"
    },
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#FBF5EA",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "NSLocationWhenInUseUsageDescription": "يستخدم التطبيق الموقع لتحديد أوقات الصلاة بدقة",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "يستخدم التطبيق الموقع لتحديد أوقات الصلاة بدقة"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#FBF5EA"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.elsline.een",
      "supportsRtl": true,
      "notification": {
        "sound": "./assets/audios/adhanVoice.mp3"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-audio",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/959de6c5-f3e3-40b1-814e-fcb498b2e579"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "959de6c5-f3e3-40b1-814e-fcb498b2e579"
      }
    },
    "owner": "elsline"
  }
}
